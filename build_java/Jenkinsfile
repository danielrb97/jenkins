pipeline{
    agent any
    stages{
        stage("unit testing patient project"){
            steps{
                dir("build_java"){
                    bat "mvn clean compile test --fail-never"
                }
            }
        }
        
    }
    sh "pwd"
    post {
        always {
            junit(testResults: 'target/surefire-reports/*.xml', allowEmptyResults : true)
        }
    }
}
